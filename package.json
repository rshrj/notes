{
  "name": "notes",
  "version": "1.0.0",
  "description": "Simple notes app",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "NODE_ENV='development' nodemon server",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rshrj/notes.git"
  },
  "keywords": [
    "Notes",
    "MERN"
  ],
  "author": "Rishi Raj <rishiraj.1012exp@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rshrj/notes/issues"
  },
  "homepage": "https://github.com/rshrj/notes#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "client": "file:client",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.0.0",
    "graphql-compose": "^7.15.0",
    "graphql-compose-mongoose": "^7.3.6",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.13",
    "normalize-url": "^5.3.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3"
  }
}
